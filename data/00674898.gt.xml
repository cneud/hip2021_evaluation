les Moncasi et les Otero.</Unicode></TextEquiv></TextRegion><TextRegion id="r3538" type="paragraph"><Coords><Point x="1264" y="1411"/><Point x="1265" y="1411"/><Point x="1265" y="1412"/><Point x="1374" y="1412"/><Point x="1374" y="1413"/><Point x="1375" y="1413"/><Point x="1375" y="1421"/><Point x="1494" y="1421"/><Point x="1494" y="1420"/><Point x="1628" y="1420"/><Point x="1628" y="1419"/><Point x="1629" y="1419"/><Point x="1629" y="1413"/><Point x="1662" y="1413"/><Point x="1662" y="1411"/><Point x="1666" y="1411"/><Point x="1666" y="1412"/><Point x="1667" y="1412"/><Point x="1667" y="1413"/><Point x="1747" y="1413"/><Point x="1747" y="1414"/><Point x="1748" y="1414"/><Point x="1748" y="1421"/><Point x="1752" y="1421"/><Point x="1752" y="1429"/><Point x="1754" y="1429"/><Point x="1754" y="1481"/><Point x="1755" y="1481"/><Point x="1755" y="1484"/><Point x="1754" y="1484"/><Point x="1754" y="1623"/><Point x="1755" y="1623"/><Point x="1755" y="1767"/><Point x="1756" y="1767"/><Point x="1756" y="1873"/><Point x="1757" y="1873"/><Point x="1757" y="1974"/><Point x="1756" y="1974"/><Point x="1756" y="2062"/><Point x="1755" y="2062"/><Point x="1755" y="2419"/><Point x="1754" y="2419"/><Point x="1754" y="2563"/><Point x="1755" y="2563"/><Point x="1755" y="2656"/><Point x="1754" y="2656"/><Point x="1754" y="2708"/><Point x="1753" y="2708"/><Point x="1753" y="2709"/><Point x="1752" y="2709"/><Point x="1752" y="2763"/><Point x="1751" y="2763"/><Point x="1751" y="2766"/><Point x="1750" y="2766"/><Point x="1750" y="2767"/><Point x="1749" y="2767"/><Point x="1749" y="2768"/><Point x="1748" y="2768"/><Point x="1748" y="2769"/><Point x="1747" y="2769"/><Point x="1747" y="2770"/><Point x="1717" y="2770"/><Point x="1717" y="2772"/><Point x="1707" y="2772"/><Point x="1707" y="2763"/><Point x="1640" y="2763"/><Point x="1640" y="2762"/><Point x="1535" y="2762"/><Point x="1535" y="2763"/><Point x="1511" y="2763"/><Point x="1511" y="2764"/><Point x="1415" y="2764"/><Point x="1415" y="2